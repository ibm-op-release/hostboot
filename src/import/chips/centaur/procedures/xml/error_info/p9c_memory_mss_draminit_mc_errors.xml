    <!-- Callout MBA LOW -->
    <callout><target>MBA</target><priority>LOW</priority></callout>
    <!-- Deconfigure MBA -->
    <deconfigure><target>MBA</target></deconfigure>
    <!-- Create GARD record for MBA -->
    <gard><target>MBA</target></gard>
</hwpError>

</hwpErrors>
